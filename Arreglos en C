#include <stdio.h>

int main()
{
    int x [] = {2,3,4};
    int *z = x;
    
    printf("El primer dato es %p \n", z);
    printf("El primer dato es %p \n", x);
    printf("El primer dato es %p \n", &x[0]);

    printf("El segundo dato es %p \n", z+1);        
    printf("El segundo dato es %p \n", x+1);
    printf("El segundo dato es %p \n", &x[1]);
    
    printf("El segundo dato es %d \n", *(z+1));        
    printf("El segundo dato es %d \n", x[1]);
    return 0;
}
