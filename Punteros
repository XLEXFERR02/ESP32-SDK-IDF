#include <stdio.h>

int main()
{
    int n = 10;
    int *p;  // int *p = &n;
    p = &n;
    
    int x[] = {2,3,4,5};
    int *a; //int *a = x;
    a = x;
    
    printf("Direcciones : %p\n",x);
    printf("Direcciones : %p\n",&x[0]);
    
    printf("Valor de indice %d es : %d\n",0,x[1]);
    printf("Valor de indice %d es : %d\n",0,*(a+1));
    
    return 0;
}
